5 7 < 
IFTHEN
   1 1 + 2 == 
   IFTHEN
      3 2 % 0 ==
      IFTHEN
        ."Math works" . CR
      ELSE
        ."Talk like Yoda, we do" . CR
      ENDIF
   ELSE
     ."Huh???" . CR
   ENDIF
ELSE 
   1 1 + 3 ==
   IFTHEN
      ."Fake News Reported" . CR
   ELSE
      ."Caught ya lying" . CR
   ENDIF
ENDIF
